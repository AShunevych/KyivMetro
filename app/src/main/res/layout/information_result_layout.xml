                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView23"
                tools:text="sometext" />

            <TextView
                android:id="@+id/bus_Info"
                style="@style/TextAppearance_Result"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView26"
                tools:text="22.56.598.126.87.9988" />

            <TextView
                android:id="@+id/trolley_info"
                style="@style/TextAppearance_Result"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView28"
                tools:text="ssadasdasd" />

            <TextView
                android:id="@+id/info_UTaxiCub"
                style="@style/TextAppearance_Result"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView30"
                tools:text="22.56.598.126.87.9988" />

            <TextView
                android:id="@+id/textView25"
                style="@style/TextAppearance_Information_HeadLine"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/Transport"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.502"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/divider5" />

        </android.support.constraint.ConstraintLayout>
    </ScrollView>
</RelativeLayout>